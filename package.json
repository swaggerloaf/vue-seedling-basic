{
  "name": "vue-seedling-basic",
  "version": "1.0.0",
  "author": "@swaggerloaf",
  "description": "Starter vue project using babel, webpack, bootstrap and VS Code",
  "scripts": {
    "start": "npm-run-all --parallel start2  start-mockapi",
    "start2": "node_modules/.bin/webpack-dev-server",
    "build": "node_modules/.bin/webpack --progress",
    "test": "jasmine",
    "generate-mock-data": "node buildScripts/generateMockData",
    "prestart-mockapi": "npm run generate-mock-data",
    "start-mockapi": "json-server --watch db.json --port 3001",
    "mock": "json-server --watch db.json --port 3001"
  },
  "license": "MIT",
  "repository": "https://github.com/swaggerloaf/react-seedling-basic",
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "bootstrap": "^4.3.1",
    "font-awesome": "^4.7.0",
    "vue": "^2.6.7"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-class-properties": "7.3.3",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-syntax-jsx": "^7.0.0",
    "@babel/plugin-transform-flow-strip-types": "^7.0.0",
    "@vue/babel-preset-app": "3.4.1",
    "babel-plugin-transform-vue-jsx": "3.7.0",
    "babel-helper-vue-jsx-merge-props": "2.0.3",
    "@babel/plugin-transform-runtime": "7.2.0",
    "@babel/register": "7.0.0",
    "@babel/preset-env": "^7.3.1",
    "babel-eslint": "~10.0.1",
    "babel-loader": "^8.0.5",
    "css-loader": "2.1.0",
    "node-sass": "4.11.0",
    "sass-loader": "7.1.0",
    "eslint": "5.14.1",
    "file-loader": "~3.0.1",
    "prettier": "^1.16.4",
    "style-loader": "0.23.1",
    "webpack": "4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.0",
    "vue-loader": "15.6.4",
    "vue-template-compiler": "2.6.7",
    "json-server": "0.14.2",
    "json-schema-faker": "0.5.0-rc16",
    "npm-run-all": "^4.1.5",
    "jasmine": "3.3.1",
    "jasmine-core": "3.3.0",
    "karma": "^4.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-safari-launcher": "^1.0.0",
    "karma-sauce-launcher": "^2.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^4.0.0-rc.2"
  },
  "engines": {
    "node": ">=8"
  },
  "keywords": [
    "vue",
    "babel",
    "webpack",
    "karma",
    "jasmine"
  ],
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "babel": {
    "presets": [
      "@vue/app"
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-transform-runtime",
      "@babel/plugin-syntax-jsx"
    ]
  }
}
